  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@godaddy/terminus": "^4.3.1",
    "@rlvt/logging": "^3.0.2",
    "await-of": "^3.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fp-ts": "^2.2.0",
    "io-ts": "^2.0.1",
    "io-ts-reporters": "^1.0.0",
    "ioredis": "^4.16.3",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.16.1",
    "@types/jest": "^25.2.1",
    "@types/pino": "^6.0.0",
    "@types/useragent": "^2.1.1",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "proxyquire": "2.1.0",
