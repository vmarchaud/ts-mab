{
  "name": "mab-next",
  "version": "2.4.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": "^12"
  },
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "dev": "TS_NODE_FILES=true ts-node -P ./tsconfig.json src/index.ts",
    "test": "cov8 ava",
